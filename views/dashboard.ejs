<%- include('partials/header'); -%>

<div class="dashboard-title">
  <h4>Мой дашборд</h4>
  <a class="btn btn-primary" href="/addnew">Добавить новый</a>
</div>

<% (blogs).forEach(blog => { %>

<table class="table table-hover dashboard">
  <tbody>
    <tr>
      <td><%= blog.title %></td>
      <td>
        <button class="btn btn-outline-primary btn-sm">Изменить</button>
        <button class="btn btn-outline-primary btn-sm">Удалить</button>
      </td>
    </tr>
  </tbody>
</table>

<% }); %> <%- include('partials/footer'); -%>
